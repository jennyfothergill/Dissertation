@article{Galindo-Murillo2015,
abstract = {Background The structure and dynamics of DNA are critically related to its function. Molecular dynamics simulations augment experiment by providing detailed information about the atomic motions. However, to date the simulations have not been long enough for convergence of the dynamics and structural properties of DNA. Methods Molecular dynamics simulations performed with AMBER using the ff99SB force field with the parmbsc0 modifications, including ensembles of independent simulations, were compared to long timescale molecular dynamics performed with the specialized Anton MD engine on the B-DNA structure d(GCACGAACGAACGAACGC). To assess convergence, the decay of the average RMSD values over longer and longer time intervals was evaluated in addition to assessing convergence of the dynamics via the Kullback-Leibler divergence of principal component projection histograms. Results These molecular dynamics simulations - including one of the longest simulations of DNA published to date at {\~{}} 44 $\mu$s - surprisingly suggest that the structure and dynamics of the DNA helix, neglecting the terminal base pairs, are essentially fully converged on the {\~{}} 1-5 $\mu$s timescale. Conclusions We can now reproducibly converge the structure and dynamics of B-DNA helices, omitting the terminal base pairs, on the $\mu$s time scale with both the AMBER and CHARMM C36 nucleic acid force fields. Results from independent ensembles of simulations starting from different initial conditions, when aggregated, match the results from long timescale simulations on the specialized Anton MD engine. General significance With access to large-scale GPU resources or the specialized MD engine "Anton" it is possible for a variety of molecular systems to reproducibly and reliably converge the conformational ensemble of sampled structures. This article is part of a Special Issue entitled: Recent developments of molecular dynamics.},
author = {Galindo-Murillo, Rodrigo and Roe, Daniel R. and Cheatham, Thomas E.},
doi = {10.1016/j.bbagen.2014.09.007},
file = {:Users/jenny/Documents/Mendeley Desktop/Galindo-Murillo, Roe, Cheatham{\_}2015{\_}Biochimica et Biophysica Acta - General Subjects.pdf:pdf},
issn = {18728006},
journal = {Biochimica et Biophysica Acta - General Subjects},
keywords = {Base pair fraying,Convergence,DNA dynamics,Molecular dynamics,Nucleic acid,Reproducibility},
number = {5},
pages = {1041--1058},
pmid = {25219455},
publisher = {Elsevier B.V.},
title = {{Convergence and reproducibility in molecular dynamics simulations of the DNA duplex d(GCACGAACGAACGAACGC)}},
url = {http://dx.doi.org/10.1016/j.bbagen.2014.09.007},
volume = {1850},
year = {2015}
}
