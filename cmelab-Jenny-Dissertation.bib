isbn = {1744-683X$\backslash$r1744-6848},
issn = {1744-683X},
journal = {Soft Matter},
keywords = {coarse-graining,ions,nice},
mendeley-tags = {coarse-graining,ions,nice},
number = {8},
pages = {2385--2397},
title = {{Self-assembly of coarse-grained ionic surfactants accelerated by graphics processing units}},
url = {http://xlink.rsc.org/?DOI=C1SM06787G http://xlink.rsc.org/?DOI=c1sm06787g http://dx.doi.org/10.1039/C1SM06787G},
volume = {8},
