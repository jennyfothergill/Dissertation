title = {{New insights into the dynamics and morphology of P3HT:PCBM active layers in bulk heterojunctions}},
url = {http://xlink.rsc.org/?DOI=c3cp53271b http://pubs.rsc.org/en/Content/ArticleLanding/2013/CP/c3cp53271b http://www.ncbi.nlm.nih.gov/pubmed/24056906},
