keywords = {DPD,MD,Material properties,Multi-scale,Polymer,Reverse-mapping},
pages = {68--77},
publisher = {Elsevier B.V.},
title = {{Multi-scale simulations for predicting material properties of a cross-linked polymer}},
url = {http://dx.doi.org/10.1016/j.commatsci.2015.02.021},
volume = {102},
year = {2015}
