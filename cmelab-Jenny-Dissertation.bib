journal = {Journal of the American Chemical Society},
number = {44},
pages = {10856--10867},
title = {{Conformational isomerism in (p-RC6H4NC)2W(dppe)2: Substantial structural changes resulting from subtle differences in the $\pi$-acidity of p-RC6H4NC}},
volume = {122},
year = {2000}
}
