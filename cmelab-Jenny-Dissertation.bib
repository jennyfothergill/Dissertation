keywords = {Dangerous interference,Precipitation,Sea level rise,Warming},
number = {6},
pages = {1704--1709},
pmid = {19179281},
title = {{Irreversible climate change due to carbon dioxide emissions}},
volume = {106},
year = {2009}
}
