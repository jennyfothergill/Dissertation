issn = {15213773},
journal = {Angewandte Chemie International Edition in English},
keywords = {Coordination modes,Isocyanide ligands,Multidentate ligands},
number = {5},
pages = {650--665},
title = {{The Coordination Chemistry of Multidentate Isocyanide Ligands}},
volume = {32},
year = {1993}
}
